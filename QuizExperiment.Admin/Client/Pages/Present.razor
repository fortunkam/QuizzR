@page "/present/{Id}"
@attribute [Authorize]
@layout AdminLayout

<PageTitle>QuizzR - Presenter</PageTitle>

<AuthorizeView>
    <Authorized>
        <QuizExperiment.Admin.Client.Shared.Present.RootComponent Id=@Id />
    </Authorized>
    <NotAuthorized>
        <RedirectToLogin />
    </NotAuthorized>
</AuthorizeView>


@code {
    [Parameter]
    public string? Id { get; set; }

}