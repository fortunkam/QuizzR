@page "/edit/{Id}"
@attribute [Authorize]
@layout AdminLayout

<PageTitle>QuizzR - Editor</PageTitle>

<AuthorizeView>
    <Authorized>
        <QuizExperiment.Admin.Client.Shared.Edit.RootComponent Id=@Id />
    </Authorized>
    <NotAuthorized>
        <RedirectToLogin />
    </NotAuthorized>
</AuthorizeView>


@code {

    [Parameter]
    public string? Id { get; set; }

}